
## read Excel file into SAS
  proc import datafile="&source\myfile.xlsx"
              out     =work.mySasData
              dbms    =xlsx replace;
              sheet   ="sheet_1"; 
              getnames=YES;
  run;

## define file name as a macro variable
  %let dlbIn = "&idbpath.\Final\Data\Indata\&indata.\&ddat";
  
  proc contents data=&dlbIn;
  run;
